pipeline {
    agent any

    stages {
        stage('build1') {
            steps {
                echo 'this is a buid stage1'
            }
        }
        stage('test1') {
            steps {
                echo 'this is a test stage1'
            }
        }
        stage('deploy1') {
            steps {
                echo 'this is deploy stage1'
            }
        }
        stage('staging1') {
            steps {
                echo 'this is deploy on staging server1'
            }
        }
        stage('production1') {
            steps {
                echo 'this is deploy on production server1'
            }
        }
    }
}
